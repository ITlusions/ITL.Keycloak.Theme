    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.8",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
